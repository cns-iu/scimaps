{
  "name": "scimaps-content-migrator",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-watch",
    "job": "npm-run-all mkindexdir index sync",
    "index": "node index-content.js",
    "mkindexdir": "mkdirp ../website/projects/scimaps/src/assets/indexes",
    "migrate-old-data": "node old-website/convert-data.js",
    "sync": "rsync -avz --delete ../content ../website/projects/scimaps/src/assets",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "watch": {
    "index": {
      "patterns": [
        "../content"
      ],
      "extensions": "md",
      "quiet": false
    },
    "sync": {
      "patterns": [
        "../content"
      ],
      "extensions": "md",
      "quiet": false
    }
  },
  "devDependencies": {
    "big-roman": "^1.0.1",
    "front-matter": "^4.0.2",
    "glob": "^7.1.6",
    "js-yaml": "^3.14.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.7.0",
    "slugify": "^1.4.6",
    "turndown": "^7.0.0"
  }
}
