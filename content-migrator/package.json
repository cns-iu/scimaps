{
  "name": "scimaps-content-migrator",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-watch",
    "job": "npm-run-all -p index sync",
    "index": "node index-content.js",
    "sync": "rsync -avz --delete ../content ../website/projects/scimaps/src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "watch": {
    "index": {
      "patterns": [
        "../content"
      ],
      "extensions": "md",
      "quiet": false
    },
    "sync": {
      "patterns": [
        "../content"
      ],
      "extensions": "md",
      "quiet": false
    }
  },
  "devDependencies": {
    "front-matter": "^4.0.2",
    "glob": "^7.1.6",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.7.0"
  }
}
